int x = 0;


void test(int really) {
    volatile int dummy;
    for (int i = 0; i < 79; i++)
      for (int j = 0; j < 34; j++)
        dummy += i -j;
  
  clflush(&really
  cpuid()


  if (really)
    // *(volatile char *)rlbuf;
    l1tf();
  }

void do_it() {
    for (int i = 0; i < 10; i++)
      test(1)

    flush(rlbuf);
    test(x);
    reload(rlbuf);
  }
